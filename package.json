  },
  "license": "MIT",
  "author": {
    "name": "Victor Truong",
    "email": "victor@victortruong.com",
    "url": "https://victortruong.com"
  },
  "main": "./build/index.js",
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rm -rf ./build && mkdir ./build",
    "dev": "nodemon -r ./node_modules/dotenv/config --watch './src/**/*.ts' --exec ts-node ./src/index.ts",
    "format": "prettier './src/**/*.ts' --write",
    "lint": "eslint './src/**/*.ts'",
    "start": "yarn build && node ./build/index.js"
  },
  "dependencies": {
    "@slack/bolt": "^2.2.3",
    "@slack/oauth": "^1.1.0",
    "@slack/types": "^1.7.0",
    "@slack/web-api": "^5.11.0",
    "ioredis": "^4.17.3",
    "lodash.samplesize": "^4.2.0",
    "mongoose": "^5.9.24",
    "randomatic": "^3.1.1"
  },
  "devDependencies": {
    "@types/ioredis": "^4.17.2",
    "@types/lodash.samplesize": "^4.2.6",
    "@types/mongoose": "^5.7.32",
    "@types/randomatic": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}

{
  "name": "whirl",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://github.com/ifvictr/whirl#readme",
  "bugs": {
    "url": "https://github.com/ifvictr/whirl/issues"
  },
  "repository": {
